# Konflux Cluster Configuration
# Copy this file to .env and update with your cluster URLs

# Konflux Production Cluster
KONFLUX_URL=https://YOUR-KONFLUX-CLUSTER:6443/
KONFLUX_KUBECONFIG=/path/to/your/konflux-kubeconfig.yaml

# Other Environments (optional)
EPHEMERAL_URL=https://YOUR-EPHEMERAL-CLUSTER:6443
STAGE_URL=https://YOUR-STAGE-CLUSTER:6443
PROD_URL=https://YOUR-PROD-CLUSTER:6443

# Default Project/Namespace
DEFAULT_PROJECT=cost-mgmt-dev-tenant

# CronJob Configuration
CRONJOB_NAME=koku-scheduled-integration-test
TEST_SCENARIO_NAME=koku-scheduled-test-job